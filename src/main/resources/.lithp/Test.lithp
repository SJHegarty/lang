<(declaration, '<@')
<(white-space, ' ')
<(semi-colon, ';')
<(tilde, '~')
<(colon, ':')
<(comma, ',')
<(star, '*')
<(assignment, ':<')

<(open-curly, '{')
<(close-curly, '}')
<(open-round, '(')
<(close-round, ')')
<(open-angled, '<')
<(close-angled, '>')
<(open-square, '[')
<(close-square, ']')
<(open-brackets, +(@OC;, @OR;, @OA;, @OS;))
<(close-brackets, +(@CC;, @CR;, @CA;, @CS;))

<(minus, '-')

~(fat-token, !\")
<(fat-string, (\"?*@FT;\"))
<(skinny-string, (\'?*!(\')\'))

<(at, '@')
<(switch-pattern, '?~')
<(switch-equals, '?=')
<(other-wise, '&!')
<(question, '?')
<(dot, '.')
<(ident, (<(seg, *[a...z])?*('-'@seg;)))
<(class, *([A...Z]?*[a...z]))
<(line-head, ('\n'?*'\t'))
<(standard-integer, (?+('-', '+')*[0...9]))
<(standard-float, (@SI;?('.'*[0...9])))